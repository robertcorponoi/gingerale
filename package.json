{
  "name": "gingerale",
  "version": "2.3.0",
  "module": "gingerale.js",
  "description": "Gingerale is a simple and easy to use spritesheet to sprites converter with support for uniform spritesheets and texture atlas' with support for tilemaps coming soon.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "rollup -c",
    "bundle:watch": "rollup -c --watch",
    "test": "node test/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertcorponoi/gingerale"
  },
  "keywords": [
    "sprite",
    "sprites",
    "spritesheet",
    "converter",
    "texture",
    "atlas"
  ],
  "typings": "lib/index.d.ts",
  "author": "Robert Corponoi",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "rollup": "^1.26.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^7.5.0",
    "typescript": "^3.6.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3"
  }
}
